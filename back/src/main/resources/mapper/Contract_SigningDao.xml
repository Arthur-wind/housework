<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.Contract_SigningDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.Contract_SigningEntity" id="contract_signingMap">
        <result property="projectName" column="project_name"/>
        <result property="content" column="content"/>
        <result property="document" column="document"/>
        <result property="signDate" column="sign_date"/>
        <result property="validityPeriod" column="validity_period"/>
        <result property="employerAccount" column="employer_account"/>
        <result property="employerName" column="employer_name"/>
        <result property="phone" column="phone"/>
        <result property="employeeAccount" column="employee_account"/>
        <result property="employeeName" column="employee_name"/>
        <result property="contactPhone" column="contact_phone"/>
        <result property="isReviewed" column="is_reviewed"/>
        <result property="reviewComment" column="review_comment"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.Contract_SigningVO" >
		SELECT * FROM contract_signing  contract_signing
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.Contract_SigningVO" >
		SELECT  contract_signing.* FROM contract_signing  contract_signing
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.Contract_SigningView" >

		SELECT  contract_signing.* FROM contract_signing  contract_signing
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.Contract_SigningView" >
		SELECT * FROM contract_signing  contract_signing <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	

</mapper>
