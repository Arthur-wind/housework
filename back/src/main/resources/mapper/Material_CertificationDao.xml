<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.Material_CertificationDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.Material_CertificationEntity" id="material_certificationMap">
        <result property="employeeAccount" column="employee_account"/>
        <result property="employeeName" column="employee_name"/>
        <result property="contactPhone" column="contact_phone"/>
        <result property="idCardImage" column="id_card_image"/>
        <result property="qualificationCertificate" column="qualification_certificate"/>
        <result property="applicationMaterials" column="application_materials"/>
        <result property="applicationReason" column="application_reason"/>
        <result property="applicationTime" column="application_time"/>
        <result property="isReviewed" column="is_reviewed"/>
        <result property="reviewComment" column="review_comment"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.Material_CertificationVO" >
		SELECT * FROM material_certification  material_certification
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.Material_CertificationVO" >
		SELECT  material_certification.* FROM material_certification  material_certification
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.Material_CertificationView" >

		SELECT  material_certification.* FROM material_certification  material_certification
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.Material_CertificationView" >
		SELECT * FROM material_certification  material_certification <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	

</mapper>
