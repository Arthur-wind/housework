<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.project_typeDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.project_typeEntity" id="project_typeMap">
        <result property="project_type" column="project_type"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.project_typeVO" >
		SELECT * FROM project_type  project_type
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.project_typeVO" >
		SELECT  project_type.* FROM project_type  project_type
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.project_typeView" >

		SELECT  project_type.* FROM project_type  project_type
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.project_typeView" >
		SELECT * FROM project_type  project_type <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	

</mapper>
