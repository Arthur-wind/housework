<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.Service_ProjectDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.Service_ProjectEntity" id="service_projectMap">
        <result property="projectName" column="project_name"/>
        <result property="projectType" column="project_type"/>
        <result property="price" column="price"/>
        <result property="coverImage" column="cover_image"/>
        <result property="status" column="status"/>
        <result property="description" column="description"/>
        <result property="employeeAccount" column="employee_account"/>
        <result property="employeeName" column="employee_name"/>
        <result property="contactPhone" column="contact_phone"/>
        <result property="isReviewed" column="is_reviewed"/>
        <result property="reviewComment" column="review_comment"/>
        <result property="thumbsUp" column="thumbs_up"/>
        <result property="thumbsDown" column="thumbs_down"/>
        <result property="last_clickTime" column="last_click_time"/>
        <result property="clickCount" column="click_count"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.Service_ProjectVO" >
		SELECT * FROM service_project  service_project
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.Service_ProjectVO" >
		SELECT  service_project.* FROM service_project  service_project
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.Service_ProjectView" >

		SELECT  service_project.* FROM service_project  service_project
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.Service_ProjectView" >
		SELECT * FROM service_project  service_project <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	

</mapper>
