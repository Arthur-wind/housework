<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.EmployeeDao">

	<!-- 可根据自己的需求，是否要使用 -->
	<resultMap type="com.entity.EmployeeEntity" id="employeeMap">
        <result property="employeeAccount" column="employee_account"/>
        <result property="password" column="password"/>
        <result property="employeeName" column="employee_name"/>
        <result property="age" column="age"/>
        <result property="gender" column="gender"/>
        <result property="contactPhone" column="contact_phone"/>
        <result property="idCard" column="id_card"/>
        <result property="photo" column="photo"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.EmployeeVO" >
		SELECT * FROM employee  employee
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.EmployeeVO" >
		SELECT  employee.* FROM employee  employee
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.EmployeeView" >

		SELECT  employee.* FROM employee  employee
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.EmployeeView" >
		SELECT * FROM employee  employee <where> 1=1 ${ew.sqlSegment}</where>
	</select>

