<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.Employer_evaluationDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.Employer_evaluationEntity" id="employer_evaluationMap">
        <result property="projectName" column="project_name"/>
        <result property="serviceEvaluation" column="service_evaluation"/>
        <result property="projectEvaluation" column="project_evaluation"/>
        <result property="satisfactionLevel" column="satisfaction_level"/>
        <result property="appointmentTime" column="appointment_time"/>
        <result property="comments" column="comments"/>
        <result property="employerAccount" column="employer_account"/>
        <result property="employerName" column="employer_name"/>
        <result property="phone" column="phone"/>
        <result property="employeeAccount" column="employee_account"/>
        <result property="employeeName" column="employee_name"/>
        <result property="contactPhone" column="contact_phone"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.Employer_evaluationVO" >
		SELECT * FROM employer_evaluation  employer_evaluation
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.Employer_evaluationVO" >
		SELECT  employer_evaluation.* FROM employer_evaluation  employer_evaluation
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.Employer_evaluationView" >

		SELECT  employer_evaluation.* FROM employer_evaluation  employer_evaluation
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.Employer_evaluationView" >
		SELECT * FROM employer_evaluation  employer_evaluation <where> 1=1 ${ew.sqlSegment}</where>
	</select>


</mapper>
