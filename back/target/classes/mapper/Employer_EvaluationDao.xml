<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.Employer_evaluationDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.Employer_evaluationEntity" id="employer_evaluationMap">
        <result property="project_name" column="project_name"/>
        <result property="service_evaluation" column="service_evaluation"/>
        <result property="project_evaluation" column="project_evaluation"/>
        <result property="satisfaction_level" column="satisfaction_level"/>
        <result property="appointment_time" column="appointment_time"/>
        <result property="comments" column="comments"/>
        <result property="employer_account" column="employer_account"/>
        <result property="employer_name" column="employer_name"/>
        <result property="phone" column="phone"/>
        <result property="employee_account" column="employee_account"/>
        <result property="employee_name" column="employee_name"/>
        <result property="contact_phone" column="contact_phone"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.Employer_evaluationVO" >
		SELECT * FROM employer_evaluation  employer_evaluation
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.Employer_evaluationVO" >
		SELECT  employer_evaluation.* FROM employer_evaluation  employer_evaluation
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.Employer_evaluationView" >

		SELECT  employer_evaluation.* FROM employer_evaluation  employer_evaluation
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.Employer_evaluationView" >
		SELECT * FROM employer_evaluation  employer_evaluation <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	

</mapper>
