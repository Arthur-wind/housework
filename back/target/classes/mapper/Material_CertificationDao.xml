<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.Material_CertificationDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.Material_CertificationEntity" id="material_certificationMap">
        <result property="employee_account" column="employee_account"/>
        <result property="employee_name" column="employee_name"/>
        <result property="contact_phone" column="contact_phone"/>
        <result property="id_card" column="id_card"/>
        <result property="application_certificate" column="application_certificate"/>
        <result property="shenqingziliao" column="shenqingziliao"/>
        <result property="application_reason" column="application_reason"/>
        <result property="application_time" column="application_time"/>
        <result property="is_reviewed" column="is_reviewed"/>
        <result property="review_comment" column="review_comment"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.Material_CertificationVO" >
		SELECT * FROM material_certification  material_certification
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.Material_CertificationVO" >
		SELECT  material_certification.* FROM material_certification  material_certification
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.Material_CertificationView" >

		SELECT  material_certification.* FROM material_certification  material_certification
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.Material_CertificationView" >
		SELECT * FROM material_certification  material_certification <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	

</mapper>
